@article{Gotze2003,
abstract = {We examine the sizes and conformations of flexible dendrimers of$\backslash$ngeneration numbers G = 4 to G = 9 by means of Monte Carlo simulations$\backslash$non a coarse-grained level. To this end, we introduce a simple, ``bead-thread{\{}''{\}}$\backslash$nmodel with variable thread-to-bead size ratio 6. In addition, we$\backslash$ncompare the results from this model to previously calculated ones,$\backslash$nbased on a soft-sphere-spring model, to test the dependence of the$\backslash$nresults on the microscopic details. We find a clear insensitivity$\backslash$nof the resulting conformational properties on those, both models$\backslash$nleading to dense-core density profiles and to increasingly compact$\backslash$nstructures upon increase of the terminal generation. Moreover, we$\backslash$ncalculate the distribution of the end monomers, which turns out to$\backslash$nbe broad and extending throughout the volume occupied by the dendrimer.$\backslash$nThe dependence of the radius of gyration R-g on the monomer number$\backslash$nN does not follow the scaling law R-g similar to N-1/3 that has been$\backslash$noften put forward in the literature, but rather it obeys, for high$\backslash$ngeneration numbers, a dependence of the form R-g similar to N-0.24.},
