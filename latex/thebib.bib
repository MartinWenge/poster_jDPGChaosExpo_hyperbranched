issn = {00219606},
journal = {Journal of Chemical Physics},
number = {19},
pages = {1--15},
pmid = {25416910},
title = {{Shear rheology and structural properties of chemically identical dendrimer-linear polymer blends through molecular dynamics simulations}},
volume = {141},
year = {2014}
}
