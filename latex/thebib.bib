doi = {10.1122/1.2807445},
file = {:home/localuser/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2008 - Chain-topology-controlled hyperbranched polyethylene as effective polymer processing aid (PPA) for extrusion of.okular:okular},
