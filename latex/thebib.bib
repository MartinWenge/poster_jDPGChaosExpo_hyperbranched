keywords = {Intrinsic viscosity,Non-Euclidean dimension,Structural transition},
month = {apr},
pages = {118--127},
publisher = {Elsevier Ltd},
title = {{Topology driven structural transition of dendrimers with a dimensional cross-over}},
url = {http://dx.doi.org/10.1016/j.polymer.2017.03.013 http://linkinghub.elsevier.com/retrieve/pii/S0032386117302604},
volume = {115},
year = {2017}
}
