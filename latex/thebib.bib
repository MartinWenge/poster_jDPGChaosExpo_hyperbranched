doi = {10.1021/ma102055w},
file = {:home/localuser/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/K{\l}os, Sommer - 2010 - Simulations of Dendrimers with Flexible Spacer Chains and Explicit Counterions under Low and Neutral pH Condition.pdf:pdf},
