journal = {Journal of Rheology},
keywords = {Catalysis,Hyperbranched polyethylene,Hyperbranched polyethylenes,Linear low density polyethylenes,Metallocene linear-low-density polyethylene,Molecular structure,Organometallics,Polymer processing aids,Rheology,Surface morphology},
month = {jan},
number = {1},
pages = {243--260},
title = {{Chain-topology-controlled hyperbranched polyethylene as effective polymer processing aid (PPA) for extrusion of a metallocene linear-low-density polyethylene (mLLDPE)}},
