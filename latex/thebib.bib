isbn = {3540344748},
issn = {14366002},
keywords = {Dendrimers,Heterogeneous catalysis,Homogeneous catalysis,Stereoselective catalysis},
number = {June},
pages = {61--96},
publisher = {Springer-Verlag},
title = {{Stereoselective dendrimer catalysis}},
