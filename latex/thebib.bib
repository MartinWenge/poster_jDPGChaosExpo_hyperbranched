issn = {0740-3194},
journal = {Magnetic resonance in medicine : official journal of the Society of Magnetic Resonance in Medicine / Society of Magnetic Resonance in Medicine},
keywords = {cascade polymers,contrast agents,dendrimers,magnetic resonance imaging},
number = {1},
pages = {1--8},
pmid = {8121264},
title = {{Dendrimer-based metal chelates: a new class of magnetic resonance imaging contrast agents.}},
volume = {31},
year = {1994}
}
