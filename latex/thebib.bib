@article{Ruelle2012a,
abstract = {The problem of hydrodynamic turbulence is reformulated as a heat flow problem along a chain of mechanical systems describing units of fluid of smaller and smaller spatial extent. These units are macroscopic but have a few degrees of freedom, and they can be studied by the methods of (microscopic) nonequilibrium statistical mechanics. The fluctuations predicted by statistical mechanics correspond to the intermittency observed in turbulent flows. Specifically, we obtain the formula $\zeta$(p)=p/3-1/In$\kappa$ In $\Gamma$(p/3 + 1) for the exponents of the structure functions (left angle bracket|$\Delta$(r)V|(p) right angle bracket {\~{}} r($\zeta$(p)). The meaning of the adjustable parameter $\kappa$ is that when an eddy of size r has decayed to eddies of size r/$\kappa$, their energies have a thermal distribution. The above formula, with (In $\kappa$)⁻¹ = .32 ± .01 is in good agreement with experimental data. This lends support to our physical picture of turbulence, a picture that can thus also be used in related problems.},
author = {Ruelle, David P},
doi = {10.1073/pnas.1218747109},
file = {:home/localuser/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ruelle - 2012 - Hydrodynamic turbulence as a problem in nonequilibrium statistical mechanics.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {dec},
number = {50},
pages = {20344--6},
pmid = {23184981},
title = {{Hydrodynamic turbulence as a problem in nonequilibrium statistical mechanics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3528496{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {109},
year = {2012}
}
