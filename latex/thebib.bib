journal = {Journal de Physique II},
month = {jan},
number = {1},
pages = {37--60},
title = {{Crossover scaling in semidilute polymer solutions: a Monte Carlo test}},
url = {http://www.edpsciences.org/10.1051/jp2:1991138},
volume = {1},
year = {1991}
}
