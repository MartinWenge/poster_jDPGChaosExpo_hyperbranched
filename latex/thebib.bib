journal = {Macromolecules},
number = {6},
pages = {1637--1644},
title = {{Polymer solutions near an interface. Adsorption and depletion layers}},
url = {http://pubs.acs.org/doi/pdf/10.1021/ma50007a007{\%}5Cnhttp://pubs.acs.org/doi/abs/10.1021/ma50007a007},
