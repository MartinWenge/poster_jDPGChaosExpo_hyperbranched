file = {:home/localuser/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Dukes et al. - 2010 - Conformational Transitions of Spherical Polymer Brushes Synthesis, Characterization, and Theory.pdf:pdf},
