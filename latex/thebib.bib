issn = {00249297},
journal = {Macromolecules},
number = {10},
pages = {3628--3641},
title = {{Coexistence of crew-cut and starlike spherical micelles composed of copolymers with an annealed polyelectrolyte block}},
volume = {39},
year = {2006}
}
